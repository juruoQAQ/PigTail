page{
  height: 100%;
}
.background{
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
  
}

.mainContainer{
  display: flex;
  height: 100%; 
  flex-direction: column;
  justify-content: space-between;
  
  background-image:url('data:image/webp;base64,');


  background-size: 100% 100%;
}

/*用户信息*/
.userInfo{
  height: 50px;
  width: 110px;
  margin-top: 20px;
  display: flex;
}
.avatar{
  height: 50px;
  width: 50px;
  border-radius: 50%;
}
.userName{
  line-height: 50px;
  margin-left: 10px;
  color: aliceblue;
}
.yourTurn{
  height: 50px;
  width: 50px;
  border-radius: 50%;
  border:1px solid rgb(61, 71, 214);
}

/*手牌区*/
.playerA{
  transform: rotate(180deg);
  display: flex;
  margin: 20px 10px 7px 10px;
}
.playerB{
  display: flex;
  margin: 20px;
}
.handCardAreaA{
  height: 100px;
  width: 250px;
  background-color: rgba(0,0,0,0);
  display: flex;
}

.numA{
  height: 5px;
  width: 200px;
  background-color: rgba(0,0,0,0);
  display: flex;
  margin: 0 15px;
}
.diomondAnum{
  width: 52px;
  height: 5px;
  margin: 0 26px;
  color: white;
}
.heartAnum{
  width: 52px;
  height: 5px;
  margin: 0 26px;
  color: white;
}
.clubAnum {
  width: 52px;
  height: 5px;
  margin: 0 26px;
  color: white;
}
.spadeAnum {
  width: 52px;
  height: 5px;
  margin:0 26px;
  color: white;
}

.handCardAreaB{
  height: 100px;
  width: 250px;
  background-color:rgba(0,0,0,0);
  display: flex;
}

.numB{
  height: 5px;
  width: 200px;
  background-color: rgba(0,0,0,0);
  display: flex;
  margin: 2px 100px;
}
.spadeBnum {
  width: 52px;
  height: 5px;
  margin: 0 26px;
  color: white;
}
.clubBnum {
  width: 52px;
  height: 5px;
  margin: 0 26px;
  color: white;
}
.heartBnum {
  width: 52px;
  height: 5px;
  margin: 0 26px;
  color: white;
}
.diomondBnum{
  width: 52px;
  height: 5px;
  margin:0 26px;
  color: white;
}

.cardSlot{
  width: 52px;
  height: 75px;
  margin: 5px;
  background-color: pink;
  border-radius: 4px;
}
/*牌堆与卡池*/
.cardArea{
  display: flex;
  justify-content: space-around;
}
.cardPool{
  height: 200px;
  width: 140px;
  display: flex;
  flex-direction: column;

}
.showArea{
  height: 200px;
  width: 140px;
  display: flex;
  flex-direction: column;
}
.cardCountsA{
  text-align: center;
  transform: rotate(180deg);
}
.cardCountsB{
  text-align: center;
}
/*按钮区域*/
.buttonShow{
  display: flex;
  justify-content: center;
  margin: 10px;
  height: 30px;
}
.buttonHide{
  margin: 10px;
  height: 30px;
}
.confirmButtonA{
  background-color:rgb(117, 117, 209);
  transform: rotate(180deg);
  margin: 5px;
}
.confirmButtonB{
  background-color:rgb(117, 117, 209);
  margin: 5px;
}

.cancelButtonA{
  background-color:rgb(163, 216, 103);
  transform: rotate(180deg);
  margin: 5px;
  box-sizing: border-box;
}
.cancelButtonB{
  background-color:rgb(163, 216, 103);
  margin: 5px;
  box-sizing: border-box;
}
.disableA{
  pointer-events: none;
  background-color:rgb(108, 108, 121);
  transform: rotate(180deg);
  margin: 5px;
  box-sizing: border-box;
}
.disableB{
  pointer-events: none;
  background-color:rgb(108, 108, 121);
  margin: 5px;
  box-sizing: border-box;
}


.image{
  width: 100%;
  height: 100%;
}
.imageTest{
  width:97%;
  height:95%;
  margin-left:1.5%;
  margin-top:1.6%;
  border-radius:5px;
  box-shadow:rgb(238, 206, 22) 0px 0px 18px inset;
}

